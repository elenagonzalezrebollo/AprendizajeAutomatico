        "def prediccion_un_caso(caso_concreto):\n",
        "  for caso in range(casos):\n",
        "    print(\"----------------------------------------------------------\")\n",
        "    print(\"PREDICCIONES CASO \", caso + 1 , \" FIGURA \", caso_concreto)\n",
        "    plt.figure(figsize=(6,3))\n",
        "    plt.subplot(1,2,1)\n",
        "    plot_image(caso_concreto, predictions[caso][caso_concreto], test_labels, \n",
        "               test_images)\n",
        "    plt.subplot(1,2,2)\n",
        "    plot_value_array(caso_concreto, predictions[caso][caso_concreto],  \n",
        "                     test_labels)\n",
        "    plt.show()\n",
        "    print(\"\\n\\n\")\n",
        "\n",
